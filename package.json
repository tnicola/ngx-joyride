{
  "name": "workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 4500",
    "build": "ng build --configuration production",
    "test": "ng test",
    "test:unit": "ng test ngx-joyride --browsers=ChromeHeadless --watch=false",
    "test:unit:ci": "CHROME_BIN=/usr/bin/google-chrome ng test ngx-joyride --browsers=ChromeHeadless --watch=false --code-coverage=true",
    "test:cypress": "node_modules/.bin/run-p -r \"start\" \"cy:run\" ",
    "cy:run": "./node_modules/.bin/wait-on http-get://localhost:4500/ && cypress run",
    "lint": "ng lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.1.1",
    "@angular/common": "~13.1.1",
    "@angular/compiler": "~13.1.1",
    "@angular/core": "~13.1.1",
    "@angular/forms": "~13.1.1",
    "@angular/platform-browser": "~13.1.1",
    "@angular/platform-browser-dynamic": "~13.1.1",
    "@angular/router": "~13.1.1",
    "@ngx-translate/core": "^14.0.0",
    "rxjs": "~6.5.4",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.1.2",
    "@angular-devkit/core": "^13.1.2",
    "@angular-devkit/schematics": "^13.1.2",
    "@angular/cli": "~13.1.2",
    "@angular/compiler-cli": "~13.1.1",
    "@angular/language-service": "~13.1.1",
    "@types/jasmine": "~3.10.3",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~18.11.2",
    "codelyzer": "^6.0.0",
    "cypress": "9.2.0",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.1.2",
    "npm-run-all": "^4.1.5",
    "ts-node": "10.4.0",
    "tslint": "~6.1.0",
    "typescript": "~4.5.4",
    "wait-on": "^6.0.0"
  }
}